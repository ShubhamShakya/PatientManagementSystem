<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:backgroundTint="@color/teal_700"
    app:cardCornerRadius="30dp">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PatientEmail"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:fontFamily="serif-monospace"
        android:textStyle="bold"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"/>
    <TextView
        android:id="@+id/cancelAppointmentDoctorPatientMail"
        android:layout_marginStart="120dp"
        android:layout_marginTop="2dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Your Text"
        android:textSize="18sp"
        android:textColor="@color/white"
        />



    <TextView

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="AppointmentDate"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:fontFamily="serif-monospace"
        android:textStyle="bold"
        android:layout_marginStart="15dp"
        android:layout_marginTop="35dp"/>
    <TextView
        android:id="@+id/cancelAppointmentDoctorAppointmentDate"
        android:layout_marginStart="150dp"
        android:layout_marginTop="29dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Your Text"
        android:textSize="18sp"
        android:textColor="@color/white"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="AppointmentTime"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:fontFamily="serif-monospace"
        android:textStyle="bold"
        android:layout_marginStart="15dp"
        android:layout_marginTop="58dp"/>

    <TextView
        android:id="@+id/cancelAppointmentDoctorAppointmentTime"
        android:layout_marginStart="150dp"
        android:layout_marginTop="53dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Your Text"
        android:textSize="18sp"
        android:textColor="@color/white"
        />

    <Button
        android:id="@+id/cancelAppointmentDoctorButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cancel"
        android:textColor="@color/teal_200"
        android:layout_marginStart="270dp"
        android:layout_marginTop="19dp"/>



</androidx.cardview.widget.CardView>